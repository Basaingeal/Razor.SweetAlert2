{
  "name": "@currietechnologies/razorsweetalert2",
  "version": "5.1.0",
  "description": "A Razor class library for interacting with SweetAlert2",
  "type": "module",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack --color --mode=production",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/ts/*.ts",
    "prettier:check": "prettier --check ./src/ts/*.ts",
    "babel": "babel ./src/ts/SweetAlert.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "mini-css-extract-plugin": "^2.5.3",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.5.5",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@sweetalert2/themes": "^5.0.8",
    "core-js": "3.21.0",
    "sweetalert2": "11.4.0"
  },
  "author": "Michael J. Currie",
  "private": true,
  "eslintIgnore": [
    "*.js",
    "*.ts"
  ],
  "browserslist": [
    "last 2 edge versions",
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "last 2 opera versions",
    "last 2 and_uc versions"
  ]
}
