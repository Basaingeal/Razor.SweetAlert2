{
  "name": "currietechnologies-razorsweetalert2",
  "version": "4.1.0",
  "description": "A Razor class library for interacting with SweetAlert2",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack --color",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/ts/*.ts",
    "prettier:check": "prettier --check ./src/ts/*.ts",
    "babel": "babel ./src/ts/SweetAlert.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "mini-css-extract-plugin": "^1.2.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.1",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.4",
    "terser-webpack-plugin": "^5.0.2",
    "typescript": "^4.0.5",
    "webpack": "^5.3.1",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "@sweetalert2/themes": "^4.0.0",
    "core-js": "3.6.5",
    "sweetalert2": "10.8.1"
  },
  "author": "Michael J. Currie",
  "private": true,
  "eslintIgnore": [
    "*.js",
    "*.ts"
  ],
  "browserslist": [
    "last 2 edge versions",
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "last 2 opera versions",
    "last 2 and_uc versions"
  ]
}
