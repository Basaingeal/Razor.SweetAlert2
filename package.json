{
  "name": "currietechnologies-razorsweetalert2",
  "version": "0.4.1",
  "description": "A Razor class library for interacting with SweetAlert2",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack -p --color",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/ts/*.ts",
    "prettier:check": "prettier --check ./src/ts/*.ts",
    "babel": "babel ./src/ts/SweetAlert.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6",
    "webpack-fix-style-only-entries": "^0.3.0"
  },
  "dependencies": {
    "@sweetalert2/themes": "^2.0.3",
    "lodash": "4.17.15",
    "sweetalert2": "8.14.0"
  },
  "author": "Michael J. Currie",
  "private": true,
  "eslintIgnore": [
    "*.js",
    "*.ts"
  ],
  "browserslist": [
    "ie 11",
    "last 2 edge versions",
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "last 2 opera versions",
    "last 2 and_uc versions"
  ]
}
