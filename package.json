{
  "name": "currietechnologies-razorsweetalert2",
  "version": "2.1.6",
  "description": "A Razor class library for interacting with SweetAlert2",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack -p --color",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/ts/*.ts",
    "prettier:check": "prettier --check ./src/ts/*.ts",
    "babel": "babel ./src/ts/SweetAlert.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-fix-style-only-entries": "^0.4.0"
  },
  "dependencies": {
    "@sweetalert2/themes": "^3.0.4",
    "core-js": "3.4.1",
    "sweetalert2": "9.3.11"
  },
  "author": "Michael J. Currie",
  "private": true,
  "eslintIgnore": [
    "*.js",
    "*.ts"
  ],
  "browserslist": [
    "last 2 edge versions",
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "last 2 opera versions",
    "last 2 and_uc versions"
  ]
}
